{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "baseUrl": "./src",
    "paths": {
      "multiformats": ["../node_modules/multiformats/dist"]
    },
    "declaration": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/*"],
  "exclude": ["src/**/*.test.ts"]
}
